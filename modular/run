#!/bin/bash
batchN=16;
mkdir $PWD/output/parruns/
sleep 2
for i in {1 .. $batchN}; do
	qsub -t 1-12 -o $PWD/output/parruns/ -p -1 runmain
	sleep 30
	while [[ $(qstat) = *runmain* ]]; do
		sleep 30
	done
done
for filename in $PWD/output/parruns/runmain*; do
	cat $filename >> $PWD/output/regWolff
done
rm -r $PWD/output/parruns/
python3 $PWD/updMaxE.py
