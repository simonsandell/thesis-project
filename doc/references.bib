@article{Fisher1973,
    abstract = {The ordered state of a d-dimensional isotropic system with an n-vector (n≥2) order parameter is considered. By the imposition of suitable boundary conditions it is shown how to define explicitly a helicity modulus ϒ(T) which measures the free-energy increment associated with "twisting" the direction of the order parameter. For a Bose system the superfluid density is seen to be $\rho$s(T)=(m/ℏ)2ϒ(T). A critical exponent v is defined by ϒ(T)∼|T-Tc|v as T→Tc; for an ideal Bose gas and spherical model (n→∞), v=1 is an exact result for all d{\textgreater}2. The difficulties of defining a correlation length in the ordered phase are discussed. A full scaling theory of the correlations avoids these problems and may be linked to a phenomenological hydrodynamic approach, to clarify and rederive Josephson's relation v=2$\beta$-$\eta$$\nu$=2-$\alpha$-2$\nu$. This reduces to v=(d-2)$\nu$ (used by some authors with d=3), only if one accepts d-dependent, "hyperscaling" relations such as d$\nu$=2-$\alpha$; however, both these latter relations fail for the ideal Bose gas when d{\textgreater}4. An alternative derivation of the formula v=2-$\alpha$-2$\nu$ is based on the scaling theory for systems with a large but finite dimension.},
    author = {Fisher, Michael E. and Barber, Michael N. and Jasnow, David},
    doi = {10.1103/PhysRevA.8.1111},
    file = {:home/simon/Downloads/Articles/PhysRevA.8.1111.pdf:pdf},
    isbn = {0556-2791},
    issn = {10502947},
    journal = {Physical Review A},
    number = {2},
    pages = {1111--1124},
    title = {{Helicity modulus, superfluidity, and scaling in isotropic systems}},
    volume = {8},
    year = {1973}
}
@article{Rudnick1977,
    abstract = {We show that Josephson's relation, $\upsilon$=2$\beta$-$\eta$$\nu$=2-$\alpha$-2$\nu$, connecting the critical exponent $\upsilon$, describing the vanishing of the helicity modulus (superfluid density), to other thermodynamic and correlation exponents is an exact scaling law within the renormalization-group framework. We use the standard isotropic n-vector model with continuous spins; when d{\textless}4 hyperscaling presumably holds, and we have $\upsilon$=(d-2)$\nu$. Secondly, we derive the first two leading terms of the helicity modulus in an $\epsilon$=4-d expansion; this allows second-order evaluation of universal amplitude ratios associated with the superfluid transition.},
    author = {Rudnick, Joseph and Jasnow, David},
    doi = {10.1103/PhysRevB.16.2032},
    file = {:home/simon/Downloads/Articles/PhysRevB.16.2032.pdf:pdf},
    issn = {01631829},
    journal = {Physical Review B},
    title = {{Renormalization-group proof of Josephson's relation and $\epsilon$ expansion for the helicity modulus (superfluid density)}},
    year = {1977}
}
