@article{Ohta1979,
    abstract = {By computing the incremental free energy of the two-dimensional XY model in a state with a long-wavelength twist of the local short-range order, we evaluate for all T{\textless}Tc the superfluid density $\rho$s for superfluid systems. Spin-wave excitations in the XY model imply a substantial (nonuniversal) depletion of $\rho$s at low temperatures. We find that these contributions do not affect the asymptotic universality; as T→Tc-, $\rho$s/T becomes universal and we recover the previous results of Nelson and Kosterlitz. In the Appendix we derive the Kosterlitz recursion relations by transforming to a sine-Gordon system and using conventional momentum shell integration techniques.},
    author = {Ohta, Takao and Jasnow, David},
